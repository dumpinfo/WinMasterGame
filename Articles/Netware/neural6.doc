\par 
\par \tab \tab \tab \} break;
\par \tab 
\par \tab \tab default:break;
\par 
\par \tab \tab \} // end swtich
\par 
\par \tab \} // end while
\par 
\par // free up resources
\par 
\par delete [] weight_matrix;
\par 
\par \} // end main}{\fs16 
